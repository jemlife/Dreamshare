<div class="posts-wrapper row">
  <% @entries.each do |entry| %>
    <div class="post">
      <div class="post-head">
        <div class="name">
          Jeremy
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag entry.image.url(:size => '200x400'), class:'img-responsive'), entry_path(entry) %>
      </div>
      <p class="caption">
        <%= entry.content %>
      </p>
    </div>
  <% end %>
</div>
